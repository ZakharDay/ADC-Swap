<% ExchangeMinor.all.each do |exchange_minor| %>
  <% if exchange_minor.profile.published %>
    <%
      profile = exchange_minor.profile
      minor = exchange_minor.minor
    %>

    <h3>
      <%= profile.first_name %>
      <%= profile.middle_name %>
      <%= profile.last_name %>
    </h3>
    <%= link_to minor.name, exchange_minor_path(exchange_minor), target: 'blank' %>

    <br>
    <br>
    whished_minor
    <br>

    <% profile.whished_minors.each do |whished_minor| %>
      <%= Minor.find(whished_minor.id).name %>
      <br>
    <% end %>
    <br>
    <br>
    <br>
  <% end %>
<% end %>
